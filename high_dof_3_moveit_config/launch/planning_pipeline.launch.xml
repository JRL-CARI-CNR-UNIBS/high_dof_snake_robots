<launch>

  <!-- This file makes it easy to include different planning pipelines;
       It is assumed that all planning pipelines are named XXX_planning_pipeline.launch  -->
  <arg name="pipeline" default="ompl" />
  <include ns="planning_pipelines/ompl" file="$(find high_dof_3_moveit_config)/launch/ompl_planning_pipeline.launch.xml" />
  <include ns="planning_pipelines/dirrt" file="$(find high_dof_3_moveit_config)/launch/dirrt_planning_pipeline.launch.xml" />

</launch>
